  <h1><%= app.set('name') %></h1>

  <div class="hummingbird_graph" id="log">

    <h2><span class="value"></span> Pageviews / Second</h2>

    <div class="axis_left"></div>
    <canvas id='page_views_canvas' width="760" height="216"></canvas>
    <div class="axis_right"></div>

  </div>

  <div style="clear: both;"></div>

  <button id="test_it" value="Send test traffic">Send test traffic</button>

  <script>
    $(document).ready(function() {
      var hummingSocket = new Hummingbird.WebSocket.Dashboard();
      hummingSocket.start();

      $("#test_it").click(function(e) {
        e.preventDefault();

        var img = document.createElement('img');
        img.src = "http://localhost:8000/tracking_pixel.gif?f=" + Math.ceil(Math.random() * 1000000)
      });
    });
  </script>
